<!-- malaria test
    <source name="malaria-gff" type="malaria-gff">
      <property name="gff3.taxonId" value="36329"/>
      <property name="gff3.seqDataSourceName" value="PlasmoDB"/>
      <property name="gff3.dataSourceName" value="PlasmoDB"/>
      <property name="gff3.seqClsName" value="Chromosome"/>
      <property name="gff3.dataSetTitle" value="PlasmoDB P. falciparum genome"/>
      <property name="src.data.dir" location="/usr/local/wormbase/intermine/data/malaria/genome/gff"/>
    </source>
-->

    <source name="celegans-gff3"              type="wormbase-gff3">
      <property name="gff3.taxonId"           value="6239"/>
      <property name="gff3.seqDataSourceName" value="WormBase"/>
      <property name="gff3.dataSourceName"    value="WormBase"/>
      <property name="gff3.seqClsName"        value="Chromosome"/>
      <property name="gff3.dataSetTitle"      value="WormBase C. elegans genome"/>
      <property name="src.data.dir"           location="/usr/local/wormbase/intermine/data/current/wormbase/c_elegans/gff3"/>
      <property name="src.data.file"          location="c_elegans.current.annotations.gff3"/>
    </source>



